{
  "plugins": [
    "@semantic-release/changelog",
    "@semantic-release/git",
    "@semantic-release/npm"
  ],
  "branches": [
    "main",
    "develop"
  ],
  "commitAnalyzer": "conventional-changelog-cli",
  "releaseRules": [
    {
      "type": "major",
      "release": "major"
    },
    {
      "type": "minor",
      "release": "minor"
    },
    {
      "type": "patch",
      "release": "patch"
    },
    {
      "type": "prerelease",
      "release": "prerelease"
    }
  ]
}
